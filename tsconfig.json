{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@services/*": ["services/*"],
      "@layouts/*": ["layouts/*"],
      "@modules/*": ["modules/*"],
      "@pages/*": ["pages/*"],
      "@util/*": ["utilities/*"]
    }
  },
  "include": ["./src"]
}
